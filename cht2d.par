[GENERAL]
numSteps	= 100000
stopAt		= numSteps
dt		= 5e-02
variableDT	= yes
targetCFL	= 0.5
writeInterval	= 20
writeControl	= timeStep
timeStepper	= bdf2

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-04
residualProj = no

[VELOCITY]
residualTol = 1e-06
density = 1.0
viscosity = 3.1623E-04

[TEMPERATURE]

conjugateHeatTransfer = yes

rhoCp = 1.0
conductivity = 3.1623E-04
residualTol = 1e-06
