[GENERAL]
numSteps	= 10000
stopAt		= numSteps
dt		= 5e-02
variableDT	= yes
targetCFL	= 0.5
writeInterval	= 50
writeControl	= timeStep
timeStepper	= bdf2

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-04
residualProj = no

[VELOCITY]
residualTol = 1e-06
density = 1.0
viscosity = 4.4722E-03

[TEMPERATURE]

conjugateHeatTransfer = yes

rhoCp = 1.0
conductivity = 6.3888E-04
residualTol = 1e-06
